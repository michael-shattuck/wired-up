{
  "name": "wired-up",
  "version": "0.1.1",
  "description": "Painless dependency injection for TypeScript",
  "main": "index.js",
  "directories": {
    "source": "src",
    "example": "example"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
